{
  "version": "0.4.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://dunna.vercel.app/",
  "keywords": [
    "random",
    "fake",
    "data",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alimehasin/dunna"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "docs": "cd docs && yarn && yarn start",
    "docs:build": "cd docs && yarn build",
    "docs:serve": "cd docs && yarn serve",
    "docs:typecheck": "cd docs && yarn typecheck"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "name": "dunna",
  "author": "Ali Mehasin",
  "module": "dist/dunna.esm.js",
  "size-limit": [
    {
      "path": "dist/dunna.cjs.production.min.js",
      "limit": "1 MB"
    },
    {
      "path": "dist/dunna.esm.js",
      "limit": "1 MB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
